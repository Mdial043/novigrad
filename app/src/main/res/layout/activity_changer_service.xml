<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".changerService">

    <Button
        android:id="@+id/upbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.849" />

    <EditText
        android:id="@+id/newview"
        android:layout_width="239dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Enter the new service Name"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.534"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.094" />

    <TextView
        android:id="@+id/Formulaire"
        android:layout_width="251dp"
        android:layout_height="29dp"
        android:background="#00BCD4"
        android:text="Formulaire Requis"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.568"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.196" />

    <EditText
        android:id="@+id/formulairesmultiline"
        android:layout_width="237dp"
        android:layout_height="127dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="Entrer les donnees requises"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="180dp" />

    <TextView
        android:id="@+id/documents"
        android:layout_width="247dp"
        android:layout_height="23dp"
        android:layout_marginStart="92dp"
        android:layout_marginLeft="92dp"
        android:background="#00BCD4"
        android:text="Documents Requis"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.518" />

    <EditText
        android:id="@+id/documentsmultiline"
        android:layout_width="236dp"
        android:layout_height="122dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="Documents Requis"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="439dp" />
</androidx.constraintlayout.widget.ConstraintLayout>